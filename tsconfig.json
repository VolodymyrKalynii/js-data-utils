{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "strict": true,
    "baseUrl": "src",
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports":true,
    "lib": [
      "es2019",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
